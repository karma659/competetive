#include<bits/stdc++.h>
using namespace std;
int main()
{
	int k,first=true;
while(scanf("%d",&k),k)
{
	if(!first)
	  printf("\n");
	  first=false;
	int arr[k];
	
	for(int i=0;i<k;i++){
	 scanf("%d" ,&arr[i]);
       }
	int a,b,c,d,e,f;
	for(a=0;a<k-5;a++){
	  for(b=a+1;b<k-4;b++){
	     for(c=b+1;c<k-3;c++){
	       for(d=c+1;d<k-2;d++){
	          for(e=d+1;e<k-1;e++){
	            for(f=e+1;f<k;f++){
	               printf("%d %d %d %d %d %d\n",arr[a],arr[b],arr[c],arr[d],arr[e],arr[f]);
	          }}}}}}


}
return 0;
}
